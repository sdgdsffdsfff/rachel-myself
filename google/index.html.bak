<script type="text/javascript">
    //切换街景和地图展示
    ;(function(){
        var btnViewArea = J.g('btnArea_map');
        var btnStreetView =  J.g('btnStreet_map')
        var content = J.g("map_block");

        if(!btnViewArea||!btnStreetView){
            return;
        }
        btnStreetView.on('click',function(){
            btnStreetView.addClass('hover');
            btnViewArea.removeClass('hover');
            content.removeClass("area_view")

        });
        btnViewArea.on('click',function(){
            btnViewArea.addClass('hover');
            btnStreetView.removeClass('hover');
            content.addClass("area_view")


        });



    })();

</script>

